<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ripple">
        <attr name="view_alpha" format="integer" />
        <attr name="view_framerate" format="integer" />
        <attr name="view_rippleDuration" format="integer" />
        <attr name="view_zoomDuration" format="integer" />
        <attr name="view_color" format="color" />
        <attr name="view_centered" format="boolean" />
        <attr name="view_type" format="enum">
            <enum name="simpleRipple" value="0" />
            <enum name="doubleRipple" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="view_ripplePadding" format="dimension" />
        <attr name="view_zoom" format="boolean" />
        <attr name="view_zoomScale" format="float" />

    </declare-styleable>
</resources>